{
  "manifest_version": 2,
  "name": "Chrome extension",
  "author": "Yauhen Prakharenka",
  "version": "1.0.1",
  "description": "Replace new tab screen with weather vidget etc.",
  "browser_action": {
    "default_icon": {
      "16": "penguin-icon.png",
      "24": "penguin-icon.png",
      "32": "penguin-icon.png",
      "48": "penguin-icon.png",
      "64": "penguin-icon.png",
      "96": "penguin-icon.png",
      "128": "penguin-icon.png"
    }
  },
  "icons": {
    "16": "penguin-icon.png",
    "32": "penguin-icon.png",
    "48": "penguin-icon.png",
    "64": "penguin-icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "geolocation",
    "history",
    "bookmarks",
    "contextMenus" 
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}